{
  "name": "digitalTwin",
  "description": "digitalTwin to protect your data!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": "logo.png"
  },
  "permissions": ["activeTab", "scripting", "tabs", "notifications"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}